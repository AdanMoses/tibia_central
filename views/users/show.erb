<div class="individual-user">
  <% if is_logged_in? %>
    <h4>LOGGED IN</h4>
  <% end %>
  <div class="user-char">
    <ul>
      <li>Character Name: <%= response["characters"]["data"]["name"] %></li>
      <li>Vocation: <%= response["characters"]["data"]["vocation"] %></li>
      <li>Level: <%= response["characters"]["data"]["level"] %></li>
      <li>World: <%= response["characters"]["data"]["world"] %></li>
      <li>Residence: <%= response["characters"]["data"]["residence"] %></li>
    </ul>
  </div>
  
  <a href="/user/<%= userid %>/edit">Edit</a>

</div>